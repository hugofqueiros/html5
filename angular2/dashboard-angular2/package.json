{
  "name": "dashboard-angular2",
  "version": "1.0.0",
  "description": "angular 2 dashboard",
  "main": "index.js",
  "scripts": {
    "build": "npm run webpack --colors --display-error-details --display-cached",
    "webpack": "webpack",
    "clean": "rimraf node_modules doc",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch",
    "server": "webpack-dev-server --inline --colors --display-error-details --display-cached --port 3000",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "preprotractor": "npm run webdriver-update",
    "protractor": "protractor",
    "e2e": "protractor",
    "ci": "npm run e2e && npm run test",
    "test": "karma start",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "start": "npm run server"
  },
  "keywords": [
    "angular2 dashboard"
  ],
  "author": "Hugo Queiros",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^0.23.0",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "object-assign": "^4.0.1",
    "phantomjs": "^1.9.19",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "angular2": "^2.0.0-alpha.48",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-alpha.12",
    "zone.js": "^0.5.8"
  }
}
